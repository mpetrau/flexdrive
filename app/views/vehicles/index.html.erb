<h1>Vehicles#index</h1>
<p>Find me in app/views/vehicles/index.html.erb</p>
<ul>
  <% @vehicles.each do |vehicle| %>
  <li>
    <h3><%= vehicle.make %> <%= vehicle.model %></h3>
    <p>Year: <b><%= vehicle.manufactureYear %></b>, KM: <b><%= vehicle.KM %></b></p>
    <%= link_to "see details", vehicle_path(vehicle) %>
  </li>
  <% end %>
</ul>
